{
  "name": "uru-youth-pc-front-stage",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "rimraf dist/production && vue-tsc -b && vite build --outDir dist/production",
    "build:staging": "rimraf dist/staging && vue-tsc -b && vite build --mode staging --outDir dist/staging",
    "clean": "rimraf dist",
    "clean:staging": "rimraf dist/staging",
    "clean:production": "rimraf dist/production",
    "preview": "vite preview --dist dist/production",
    "preview:staging": "vite preview --mode staging --dist dist/staging"
  },
  "dependencies": {
    "axios": "^1.10.0",
    "element-plus": "^2.10.4",
    "js-cookie": "^3.0.5",
    "lodash-unified": "^1.0.3",
    "nprogress": "^0.2.0",
    "pinia": "^3.0.3",
    "qs": "^6.14.0",
    "vue": "^3.5.17",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@types/js-cookie": "^3.0.6",
    "@types/node": "^24.0.12",
    "@types/nprogress": "^0.2.3",
    "@types/qs": "^6.14.0",
    "@vitejs/plugin-vue": "^6.0.0",
    "@vue/tsconfig": "^0.7.0",
    "autoprefixer": "^10.4.21",
    "cssnano": "^7.1.0",
    "postcss": "^8.5.6",
    "rimraf": "^6.0.1",
    "sass": "^1.89.2",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.8.3",
    "vite": "^7.0.3",
    "vue-tsc": "^2.2.12"
  }
}
